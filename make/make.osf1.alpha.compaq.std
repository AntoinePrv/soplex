#--- $Id: make.osf1.alpha.compaq.std,v 1.5 2003/01/15 17:26:05 bzfkocht Exp $
CXX	=	cxx
CPPFLAGS=	-Isrc -DNDEBUG -DWITH_ZLIB
CXXFLAGS=	-std strict_ansi -O2 -arch host -nortti \
		-ansi_alias -inline speed -assume accuracy_sensitive -ieee -g \
		-ptr $(OBJDIR)/cxx_repository   
REPOSIT	=	$(OBJDIR)/cxx_repository/*.o
LDFLAGS	=	-non_shared -L/usr/local/lib -lz -lm
ARFLAGS	=	crs
