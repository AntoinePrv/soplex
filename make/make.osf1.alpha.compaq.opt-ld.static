#--- $Id: make.osf1.alpha.compaq.opt-ld.static,v 1.3 2006/02/01 16:26:31 bzfhille Exp $
CXX	=	cxx
CPPFLAGS=	-Isrc -I/usr/local/include -DNDEBUG -DNO_ADDITIONAL_CHECKS \
                -DWITH_LONG_DOUBLE -DWITH_ZLIB
CXXFLAGS=	-std strict_ansi -O4 -fast -distinguish_nested_enums -nortti -nocleanup \
		-ptr $(OBJDIR)/cxx_repository   
REPOSIT	=	$(OBJDIR)/cxx_repository/*.o
LDFLAGS	=	-non_shared -L/usr/local/lib -lz -lm -spike
ARFLAGS	=	crs 
